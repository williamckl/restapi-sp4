	<build>
		<sourceDirectory>src</sourceDirectory>
		<resources>
			<resource>
				<directory>src/</directory>
				<excludes>
					<exclude>**/*.java</exclude>
				</excludes>
			</resource>
		</resources>
		<plugins>
			<plugin>
				<artifactId>maven-war-plugin</artifactId>
				<version>2.6</version>
				<configuration>
					<warSourceDirectory>WebContent</warSourceDirectory>
					<failOnMissingWebXml>false</failOnMissingWebXml>
					<warName>aa-restapi</warName>
					<dependentWarExcludes>
                            WEB-INF/lib/log4j-1.2.15.jar,WEB-INF/lib/jsr311-api-1.1.1.jar,WEB-INF/lib/xmlbeans-2.6.0.jar,WEB-INF/lib/commons-codec-1.2.jar,WEB-INF/lib/gson-2.1.jar,WEB-INF/lib/commons-io-2.1.jar,WEB-INF/lib/httpclient-4.5.3.jar,WEB-INF/lib/xalan-2.7.0.jar
                    </dependentWarExcludes>
				</configuration>
			</plugin>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.3</version>
				<configuration>
					<source>1.7</source>
					<target>1.7</target>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.axis2</groupId>
				<artifactId>axis2-wsdl2code-maven-plugin</artifactId>
				<version>1.7.9</version>
				<executions>
				    <execution>
						<id>GENERATE_ADMIN_ANA_SVC_STUB</id>
						<phase>generate-sources</phase>
						<configuration>
							<packageName>com.arcot.euds.anamgmt.ws</packageName>
							<databindingName>adb</databindingName>
							<unpackClasses>true</unpackClasses>
							<suppressPrefixes>true</suppressPrefixes>
							<namespaceToPackages>
								http://ws.arcot.com/UserSchema/1.0=com.arcot.euds.anamgmt.ws.schema,http://ws.arcot.com/ArcotAnARegistrySvc/1.0=com.arcot.euds.anamgmt.ws.msgs,http://ws.arcot.com/ArcotAnARegistrySvc/1.0/msgs=com.arcot.euds.anamgmt.ws.msgs
							</namespaceToPackages>
							<wsdlFile>${project.basedir}/../common/platform/euds/webservice/wsdl/ArcotAnARegistrySvc.wsdl</wsdlFile>
							<outputDirectory>${axis2output}</outputDirectory>
						</configuration>
						<goals>
							<goal>wsdl2code</goal>
						</goals>
					</execution>
					<execution>
						<id>GENERATE_UDS_STUB</id>
						<phase>generate-sources</phase>
						<configuration>
							<outputDirectory>${axis2output}</outputDirectory>
							<packageName>com.arcot.euds.wsclient.udssvcstub</packageName>
							<wsdlFile>${project.basedir}/../common/platform/euds/webservice/wsdl/ArcotUserManagementSvc.wsdl</wsdlFile>
							<databindingName>adb</databindingName>
						</configuration>
						<goals>
							<goal>wsdl2code</goal>
						</goals>
					</execution>
					<execution>
						<id>GENERATE_ORG_MANAGEMENT_SVC_STUB</id>
						<phase>generate-sources</phase>
						<configuration>
							<packageName>com.arcot.euds.orgmgmt.ws</packageName>
							<databindingName>adb</databindingName>
							<unpackClasses>true</unpackClasses>
							<suppressPrefixes>true</suppressPrefixes>
							<namespaceToPackages>
								http://ws.arcot.com/UserSchema/1.0=com.arcot.euds.orgmgmt.ws.schema,http://ws.arcot.com/ArcotUserRegistryMgmtSvc/1.0/msgs=com.arcot.euds.orgmgmt.ws.msgs,http://ws.arcot.com/ArcotUserRegistrySvc/1.0/msgs=com.arcot.euds.orgmgmt.ws.msgs
							</namespaceToPackages>
							<wsdlFile>${project.basedir}/../common/platform/euds/webservice/wsdl/ArcotOrganizationManagementSvc.wsdl</wsdlFile>
							<outputDirectory>${axis2output}</outputDirectory>
						</configuration>
						<goals>
							<goal>wsdl2code</goal>
						</goals>
					</execution> 
					<execution>
						<id>GENERATE_ADMIN_STUB</id>
						<phase>generate-sources</phase>
						<configuration>
							<outputDirectory>${axis2output}</outputDirectory>
							<packageName>com.arcot.webfort.wsclient.stub.admin</packageName>
							<wsdlFile>${project.basedir}/../auth/cygnets2/webfort/impl/wsdl/6.3/ArcotWebFortAdminSvc.wsdl</wsdlFile>
							<databindingName>adb</databindingName>
							<unpackClasses>true</unpackClasses>
							<suppressPrefixes>true</suppressPrefixes>
							<namespaceToPackages>${ns2pkgmap}</namespaceToPackages>
						</configuration>
						<goals>
							<goal>wsdl2code</goal>
						</goals>
					</execution>
					<execution>
						<id>GENERATE_RISK_ADMIN_STUB</id>
						<phase>generate-sources</phase>
						<configuration>
							<outputDirectory>${axis2output}</outputDirectory>
							<packageName>com.arcot.admin.wsclient.riskfort.arcotriskfortadminsvcstub</packageName>
							<wsdlFile>${project.basedir}/../risk/cygnets2/riskfort/impl/wsdl/8.1/ArcotRiskFortAdminWebSvc.wsdl</wsdlFile>
							<databindingName>adb</databindingName>
						</configuration>
						<goals>
							<goal>wsdl2code</goal>
						</goals>
					</execution>   
				</executions>
			</plugin>
			<plugin>
				<artifactId>maven-antrun-plugin</artifactId>
				<version>1.1</version>
				<executions>
					<execution>
						<id>MOVE_GENERATED_SOURCES</id>
						<phase>generate-sources</phase>
						<configuration>
							<tasks>
								<move todir="src">
									<fileset dir="${axis2output}/src">
										<include name="**/*" />
									</fileset>
								</move>
							</tasks>
						</configuration>
						<goals>
							<goal>run</goal>
						</goals>
					</execution>
				</executions>
			</plugin>

		</plugins>
	</build>


